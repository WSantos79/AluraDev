                                const compose = (...fns) => res => fns.reduce((accum, next)<br> => next(accum), res)<br><br>
                                
                                const unfold = (f, seed) => {<br>
                                    const go = (f, seed, acc) => {<br>
                                    const res = f(seed)<br>
                                    return res ? go(f, res[1], acc.concat([res[0]])) : acc<br>
                                    }<br>
                                    return go(f, seed, [])<br>
                                }</p>                            
